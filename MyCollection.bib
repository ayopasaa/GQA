author = {Guimar{\~{a}}es, J.R.D. and Fostier, A.-H. H and Forti, M.C. C and Melfi, J.A. A and Kehrig, H. and Mauro, J.B.N. B N and Malm, O. and Krug, J.F. F and Guimaraes, J R D and Fostier, A.-H. H and Forti, M.C. C and Melfi, J.A. A and Kehrig, H. and Mauro, J.B.N. B N and Malm, O. and Krug, J.F. F},
issn = {0044-7447},
journal = {AMBIO},
month = {jun},
number = {4},
pages = {296--301},
publisher = {SPRINGER},
title = {{Mercury in human and environmental samples from two lakes in Amapa, Brazilian Amazon}},
type = {Article},
volume = {28},
year = {1999}
}
