issn = {0747-7309},
journal = {JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES},
month = {dec},
number = {D23},
pages = {28895--28905},
publisher = {AMER GEOPHYSICAL UNION},
title = {{Rainfall chemistry composition in two ecosystems in the northeastern Brazilian Amazon (Amapa State)}},
type = {Article},
url = {http://doi.wiley.com/10.1029/2000JD900235},
volume = {105},
year = {2000}
}
@article{ISI:A1997WP60200008,
abstract = {An on-line microwave-assisted decomposition procedure for the
determination of lead in fish is proposed. 300 mu L slurry and 400 mu L
of a 6 mol L(-1) HNO3 solution were simultaneously injected, and the
mixture was positioned inside a microwave oven. The decomposed sample
inside the flushing solution was collected in the autosampler cup of a
graphite furnace. The proposed procedure covered the 2.5 to 25 mu g
L(-1) Pb range and presented a detection limit of 0.72 mu g L(-1) Pb.
Precision expressed as RSD, was 10.5{\%} (n = 20) for repeatability and
14.3{\%} (n = 10) for reproducibility. Accuracy was assessed using
standard reference material, and also by comparing the results to a
nitric-perchloric decomposition procedure.},
