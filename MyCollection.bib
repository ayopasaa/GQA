@article{ISI:000088410200030,
abstract = {In the present work, results from a survey on mercury concentration in
sediments and soils from a gold mining area along the Vila Nova river,
in Amapa State, Brazil, are presented. These values were compared with
those from the Igarape: Pedra Preta basin, an area unaffected by mining
activities. Total mercury contents were determined in the muddy
