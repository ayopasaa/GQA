@article{Fostier2006,
abstract = {The concentrations of Total Gaseous Mercury (TGM) and Total Particulate Mercury (TPM) were monitored during the 2002-2003 rainy and dry seasons at two sampling points (industrial and residential areas) of the Campinas Metropolitan Region. No significant difference was observed between the concentrations found at the two sampling areas and the mean values were 7.0 ± 5.8 ng m -3 (TGM) and 0.4 ± 0.3 ng m-3 (TPM). The analysis of the diel variability showed higher TGM concentrations during the day, which could be related to more intense anthropogenic activity during the day. Atmospheric dispersion processes could also explain some seasonal variation observed in TGM concentrations. For TPM concentrations a decreasing trend was observed during the rainy season, which could be explained by the removal of particles by wet deposition. The concentrations of TGM and TPM found in this study were of the same order of magnitude of those recorded in some highly industrialized regions of the northern hemisphere. These data show that emissions from the most industrialized Brazilian regions, and probably from similar regions in other countries of the southern hemisphere, should also be assessed and integrated into the global anthropogenic mercury emission assessment. {\textcopyright}2006 Sociedade Brasileira de Qu{\'{i}}mica.},
author = {Fostier, Anne H. A.H. and Michelazzo, P.A.M. A M},
issn = {01035053},
journal = {Journal of the Brazilian Chemical Society},
number = {5},
pages = {886--894},
title = {{Gaseous and particulate atmospheric mercury concentrations in the Campinas Metropolitan Region (S{\~{a}}o Paulo State, Brazil)}},
volume = {17},
year = {2006}
}
@article{Tomazelli2003,
abstract = {Studies were carried out to investigate the contamination of Piracicaba and Mogi-Gua{\c{c}}u River basins (S{\~{a}}o Paulo State, Southeastern, Brazil) for heavy metals (Cd and Pb), using the limnic bivalve Anodontites trapesialis as a biological monitor. The results showed that the concentrations of Pb were higher than the control group in both basins, showing the bioavailability of this non-essential element in the basins. The concentrations were higher in the Mogi-Gua{\c{c}}u than in the Piracicaba basin, and in the slightly contaminated sites in both basins. There was no correlation between the degree of human impact and Cd and Pb concentrations, it was not possible to infer about concentrations of these heavy metals in the bivalves based only in a broad evaluation of human impact.},
author = {Tomazelli, Andr{\'{e}}a C. A.C. and Martinelli, Luiz A. L.A. and Avelar, Wagner E P W.E.P. and {De Camargo}, P.B. Pl{\'{i}}nio B. and Fostier, A.-H. Anne Helene and Ferraz, Epaminondas S B E.S.B. and Krug, Francisco J. F.J. and Santos, D{\'{a}}rio and {Santos Jr.}, D.},
